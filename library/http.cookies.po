#: ../Doc/library/http.cookies.rst:135
msgid "``path``"
msgstr "``path``"

#: ../Doc/library/http.cookies.rst:136
msgid "``comment``"
msgstr "``comment``"

#: ../Doc/library/http.cookies.rst:137
msgid "``domain``"
msgstr "``domain``"

#: ../Doc/library/http.cookies.rst:138
msgid "``max-age``"
msgstr "``max-age``"

#: ../Doc/library/http.cookies.rst:139
msgid "``secure``"
msgstr "``secure``"

